{
  "mappings": [
    {
      "src": "./scss",
      "dest": "./style"
    }
  ],
  "ignores": ["*.css"],
  "options": {
    "compassMode": true,
    "sourceMap": false,
    "lineComments": false,
    "debugInfo": false,
    "unixNewlines": true,
    "outputStyle": "compressed",
    "autoprefixConfig": "> 1%, last 2 versions, Firefox ESR, Opera 12.1",
    "autoprefix": true
  },
  "customOptions": [],
  "includePaths": [],
  "requireLibs": []
}
